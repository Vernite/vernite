{
  "files.associations": {
    ".xtm": "xml",
    ".ngI18nconfig": "json"
  },
  "eslint.validate": [
    "javascript",
    "typescript",
    "html"
  ],
  "eslint.options": {
    "extensions": [
      ".js",
      ".ts",
      ".html"
    ]
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  },
  "editor.formatOnSave": true,

  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },

  "stylelint.enable": true,
  "scss.validate": false,
  "css.validate": false,
  "less.validate": false,

  "stylelint.validate": [
    "scss"
  ],

  "explorer.compactFolders": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    ".stylelintrc.json": true,
    ".browserslistrc": true,
    ".editorconfig": true,
    ".eslintrc.json": true,
    ".gitattributes": true,
    ".gitignore": true,
    ".ngI18nconfig": true,
    ".prettierrc.js": true,
    "cypress.json": true,
    "package-lock.json": true,
    "LICENSE.md": true,
    "tailwind.config.js": true,
    "tsconfig.app.json": true,
    "tsconfig.doc.json": true,
    "tsconfig.json": true,
    "tsconfig.spec.json": true,
    "yarn.lock": true,
    "karma.conf.js": true,
    "documentation": true,
    ".github": true,
    ".angular": true,
    "node_modules": true
  },
  "explorerExclude.backup": null
}
